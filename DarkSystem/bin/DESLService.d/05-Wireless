#!/bin/sh
#//////////////////////////////////////////////////
#//DESLinux DESLService.d :: System Initializer
#//	(C)2014-2025 Dark Embedded Systems.
#//	http://e.xprj.net/
#//////////////////////////////////////////////////
SELF=`readlink -f ${0}`
sRoot=${SELF%/*};

Main(){
	${sRoot}/../ShowState 4

	"${sRoot}/../WiFiConfig" || {
		case "${?}" in
			250)
				echo 'NOT INSTALLED' >&9
				return 250
			;;
			*)
				echo 'WARNING' >&9
				return 250;
			;;
		esac
	}

	return 0;
}

Main "${@}";
