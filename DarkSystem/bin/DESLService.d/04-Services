#!/bin/sh
#//////////////////////////////////////////////////
#//DESLinux DESLService.d :: System Initializer
#//	(C)2014-2025 Dark Embedded Systems.
#//	http://e.xprj.net/
#//////////////////////////////////////////////////
SELF=`readlink -f ${0}`
sRoot=${SELF%/*};

Main(){
	${sRoot}/../ShowState 3

	"${DROOT:-/DRoot}/ServerLoader" auto || {
		case "${?}" in
			250) return 250;;
		esac
	}

	return 0;
}

Main "${@}";
